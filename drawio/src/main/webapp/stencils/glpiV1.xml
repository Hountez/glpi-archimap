		<foreground>
			<fillstroke/>
			<path>
				<move x="0" y="10"/>
				<arc rx="30" ry="10" x-axis-rotation="0" large-arc-flag="0" sweep-flag="0" x="60" y="10"/>
			</path>
			<stroke/>
		</foreground>
